Host *
  IdentityFile ~/.ssh/id_rsa
  AddKeysToAgent yes
  ServerAliveInterval 60

Host gh
  HostName github.com
  User git

Host bb
  HostName bitbucket.org
  User git

Host university
  HostName university.bobsomers.net
  User bsomers
  ForwardAgent yes

Host tarbean
  HostName tarbean.bobsomers.net
  User root
  ForwardAgent yes

Host kvothe
  HostName kvothe.bobsomers.net
  User bsomers

Host ademre
  HostName ademre.bobsomers.net
